{
  "name": "injective-helix",
  "version": "1.10.3",
  "description": "The Premier Decentralized Crypto Exchange",
  "license": "Apache-2.0",
  "author": "@InjectiveLabs",
  "private": true,
  "browser": {
    "buffer": true
  },
  "scripts": {
    "fetch:data": "ts-node -P tsconfig-script.json ./scripts/marketMap.ts && ts-node -P tsconfig-script.json ./scripts/tokens.ts && ts-node -P tsconfig-script.json ./scripts/restriction.ts && ts-node -P tsconfig-script.json ./scripts/swap.ts && ts-node -P tsconfig-script.json ./scripts/denom.ts",
    "github:version": "ts-node -P tsconfig-script.json ./scripts/github.ts",
    "dev": "nuxi dev",
    "dev:local": "LOCAL_LAYER=true nuxi dev",
    "build": "nuxi build",
    "start": "nuxi start",
    "generate": "NODE_OPTIONS=--max-old-space-size=30720 nuxt generate",
    "generateNoSourceMap": "BUILD_SOURCEMAP=false NODE_OPTIONS=--max-old-space-size=30720 nuxt generate",
    "postinstall": "yarn add https://github.com/InjectiveLabs/shared-packages#master --ignore-scripts && yarn fetch:data && yarn github:version",
    "analyze": "nuxi build --analyze",
    "preview": "nuxi preview",
    "clean-up": "rm -rf dist && rm -rf .output && rm -rf .nuxt && rm -rf ./node_modules/.c12 && rm -rf ./node_modules/.cache/vite && rm -rf ./node_modules/.vite && rm -rf ./node_modules/.cache/vite",
    "lint": "eslint --max-warnings 0 '**/*.{js,ts,vue,html}' --fix && stylelint '**/*.{css,.vue}'",
    "coverage": "yarn jest --coverage",
    "prepare": "husky",
    "typecheck": "nuxi typecheck"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html}": [
      "eslint --max-warnings 0 --no-ignore"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@funken-studio/sitemap-nuxt-3": "^4.0.4",
    "@nuxt/ui": "^2.18.7",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sitemap": "^2.4.0",
    "@popperjs/core": "^2.11.6",
    "apexcharts": "^4.0.0",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "core-js": "^3.10.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "deepmerge": "^4.2.2",
    "floating-vue": "2.0.0-beta.24",
    "gsap": "^3.12.5",
    "html-to-image": "^1.11.11",
    "lightweight-charts": "^4.1.1",
    "lottie-web": "^5.12.2",
    "mixpanel-browser": "^2.48.1",
    "shared-packages": "https://github.com/InjectiveLabs/shared-packages#master",
    "simple-git": "3.27.0",
    "tailwind-merge": "^2.5.2",
    "vue-jest": "^4.0.0-0"
  },
  "devDependencies": {
    "@bangjelkoski/node-stdlib-browser": "^0.0.1",
    "@bangjelkoski/vite-plugin-node-polyfills": "^0.0.2",
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@injectivelabs/eslint-config": "^1.1.8",
    "@injectivelabs/node-globals-polyfill": "0.1.6",
    "@injectivelabs/prettier-config": "^1.0.3",
    "@injectivelabs/stylelint-config": "1.1.3",
    "@nuxt/typescript-build": "^3.0.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.197",
    "@types/mixpanel-browser": "^2.47.5",
    "@types/node": "^20.4.9",
    "@types/vue-select": "^3.16.2",
    "autoprefixer": "latest",
    "eslint": "8.37.0",
    "husky": "9.1.6",
    "jest": "^29.4.3",
    "lint-staged": "15.4.3",
    "postcss": "8.4.27",
    "postcss-custom-properties": "^13.3.0",
    "postcss-html": "^1.5.0",
    "prettier": "3.0.0",
    "stylelint": "15.10.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "~9.4.2",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4",
    "unplugin-auto-import": "^0.16.0",
    "vite-plugin-circular-dependency": "^0.2.1",
    "vite-plugin-notifier": "latest",
    "vite-tsconfig-paths": "4.2.0",
    "vue-tsc": "2.1.6"
  },
  "resolutions": {
    "@solana/web3.js": "1.98.0",
    "rpc-websockets": "9.0.4",
    "**/libsodium": "npm:@bangjelkoski/noop",
    "**/libsodium-wrappers": "npm:@bangjelkoski/noop",
    "@ethereumjs/tx": "^4.1.1",
    "eslint": "8.37.0"
  }
}
